5.
public class TypeCastingExample {
    public static void main(String[] args) {
        // Widening Conversion
        int numInt = 100;
        long numLong = numInt; // Implicit casting from int to long
        System.out.println("Int value: " + numInt);
        System.out.println("Long value: " + numLong);
        
        // Narrowing Conversion
        double numDouble = 123.456;
        int numInt2 = (int) numDouble; // Explicit casting from double to int
        System.out.println("Double value: " + numDouble);
        System.out.println("Int value: " + numInt2);
    }
}
